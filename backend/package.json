    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
